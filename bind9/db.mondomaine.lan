$TTL	604800
@	IN	SOA	ns.mondomaine.lan admin.mondomaine.lan. (
	     2411201436	    ; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; enregistrement des noms
@	IN	NS	ns.mondomaine.lan.
ns  IN  A   172.16.1.1
VM1  IN  A  172.16.1.2
site1 IN CNAME ns
site2 IN CNAME ns


